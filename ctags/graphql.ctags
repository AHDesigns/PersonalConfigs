--langdef=graphql
--langmap=graphql:.graphql
--regex-graphql=/^type[ \t]*([a-zA-Z0-9_]+)/\1/t,type/
--regex-graphql=/^enum[ \t]*([a-zA-Z0-9_]+)/\1/e,enum/
--regex-graphql=/^interface[ \t]*([a-zA-Z0-9_]+)/\1/i,interface/
--regex-graphql=/^query[ \t]*([a-zA-Z0-9_]+)/\1/q,query/
--regex-graphql=/^mutation[ \t]*([a-zA-Z0-9_]+)/\1/m,mutation/
--regex-graphql=/^fragment[ \t]*([a-zA-Z0-9_]+)/\1/f,fragment/

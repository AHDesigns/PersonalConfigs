{
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors": {
        "normal": ["#8FBCBB", "#434C5E"],
        "insert": "#BF616A",
        "visual": "#B48EAD",
        "visualline": "#B48EAD",
        "visualblock": "#A3BE8C",
        "replace": "#D08770"
    },
    "editor.cursorBlinking": "blink",
    "editor.fontSize": 12,
    "editor.parameterHints": false,
    "editor.renderIndentGuides": true,
    // "editor.tabSize": 4,
    "scss.validate": false,
    "workbench.editor.enablePreview": false,
    "git.confirmSync": false,
    "window.titleBarStyle": "custom",
    "terminal.integrated.fontSize": 14,
    "editor.renderLineHighlight": "all",
    "editor.rulers": [100],
    "workbench.editor.openPositioning": "last",
    "files.insertFinalNewline": true,
    "window.newWindowDimensions": "maximized",
    "editor.formatOnType": true,
    // "search.exclude": {
    //   "**/node_modules": true,
    //   "**/bower_components": true,
    //   "**/tmp": true,
    //   "**/dist": true,
    //   "**/.git": true,
    //   "**/.svn": true,
    //   "**/.hg": true,
    //   "**/CVS": true,
    //   "**/.DS_Store": true
    // },
    "window.title": "${rootName}${separator}${activeEditorMedium}",
    "editor.wordWrap": "on",
    "editor.dragAndDrop": true,
    "terminal.integrated.cursorBlinking": false,
    "editor.minimap.enabled": false,
    "git.enableSmartCommit": true,
    "editor.autoIndent": true,
    "workbench.startupEditor": "newUntitledFile",
    "window.restoreWindows": "all",
    "window.openFoldersInNewWindow": "on",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false
    },
    "explorer.openEditors.visible": 0,
    "workbench.editor.showTabs": true,
    "window.nativeTabs": false,
    "window.zoomLevel": 0,
    "files.useExperimentalFileWatcher": true,
    "editor.smoothScrolling": true,
    "workbench.colorCustomizations": {
        "tab.activeBorder": "#73add8",
        "badge.background": "#73add8",
        "badge.foreground": "#ffffff",
        "activityBarBadge.background": "#73add8",
        "activityBarBadge.foreground": "#ffffff",
        "editorCursor.foreground": "#73add8",
        "editorLineNumber.foreground": "#4a4a4a",
        "focusBorder": "#73add8",
        "editorIndentGuide.activeBackground": "#aaaaaa",
        "statusBar.background": "#8FBCBB",
        "statusBar.noFolderBackground": "#8FBCBB",
        "statusBar.debuggingBackground": "#8FBCBB",
        "statusBar.foreground": "#434C5E"
    },
    "editor.cursorStyle": "block",
    "editor.find.seedSearchStringFromSelection": false,
    "workbench.iconTheme": "vscode-icons",
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "[php]": {},
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.editor.labelFormat": "short",
    "files.exclude": {
      "node_modules/": true,
      "**/*.swp": true,
      "tags": true,
      ".idea/": true,
      ".vscode/": true,
    }
  }
